  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "health-check": "tsx scripts/health-check.ts",
    "lint": "next lint",
